        <UML:Attribute isSpecification="false" visibility="public" xmi.id="5648" type="5647" name="aConstraint" />
        <UML:Attribute comment="Table of WHERE clause constraints" isSpecification="false" visibility="public" xmi.id="5649" type="5640" name="nOrderBy" />
